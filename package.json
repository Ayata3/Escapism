    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
