    "gatsby-plugin-postcss": "6.9.0",
    "gatsby-plugin-purgecss": "6.2.1",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sharp": "5.9.0",
    "gatsby-plugin-sitemap": "6.9.0",
    "gatsby-plugin-twitter": "5.9.0",
    "gatsby-plugin-typescript": "5.9.0",
